{
  "dashboard": {
    "id": null,
    "title": "Feature Store Performance",
    "tags": ["feature-store", "api", "performance"],
    "timezone": "UTC",
    "panels": [
      {
        "id": 1,
        "title": "API Response Times",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(feature_store_request_duration_seconds_bucket[5m]))*1000",
            "legendFormat": "P95 Latency (ms)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(feature_store_request_duration_seconds_bucket[5m]))*1000",
            "legendFormat": "P50 Latency (ms)",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 30},
                {"color": "red", "value": 40}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(feature_store_requests_total[1m])",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Requests per second",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Cache Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(feature_store_cache_operations_total{result=\"hit\"}[1m])",
            "legendFormat": "Cache Hits/sec",
            "refId": "A"
          },
          {
            "expr": "rate(feature_store_cache_operations_total{result=\"miss\"}[1m])",
            "legendFormat": "Cache Misses/sec",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Cache Hit Ratio",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(feature_store_cache_operations_total{result=\"hit\"}[5m]) / (rate(feature_store_cache_operations_total{result=\"hit\"}[5m]) + rate(feature_store_cache_operations_total{result=\"miss\"}[5m]))",
            "legendFormat": "Hit Ratio",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.8},
                {"color": "green", "value": 0.9}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Database Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(feature_store_db_latency_seconds_bucket[5m]))*1000",
            "legendFormat": "DB P95 Latency (ms)",
            "refId": "A"
          },
          {
            "expr": "rate(feature_store_db_operations_total{result=\"success\"}[1m])",
            "legendFormat": "DB Operations/sec",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "Error Rates",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(feature_store_requests_total{status=~\"5..\"}[1m])",
            "legendFormat": "5xx Errors/sec",
            "refId": "A"
          },
          {
            "expr": "rate(feature_store_requests_total{status=~\"4..\"}[1m])",
            "legendFormat": "4xx Errors/sec",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 7,
        "title": "gRPC vs REST Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(feature_store_grpc_latency_seconds_bucket[5m]))*1000",
            "legendFormat": "gRPC P95 Latency (ms)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(feature_store_request_duration_seconds_bucket{endpoint=\"/api/v1/features/user\"}[5m]))*1000",
            "legendFormat": "REST P95 Latency (ms)",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}